{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "name": "Article",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "Title": {
      "type": "string"
    },
    "Body": {
      "type": "richtext"
    },
    "Gallery": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false
    },
    "user": {
      "via": "articles",
      "plugin": "users-permissions",
      "model": "user"
    }
  }
}
